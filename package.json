    "start:mock": "cross-env BUILD_ENV=mock max dev",
    "build:dev": "cross-env BUILD_ENV=dev max build",
    "build:debug": "cross-env BUILD_ENV=debug max build",
    "build:test": "cross-env BUILD_ENV=test max build",
