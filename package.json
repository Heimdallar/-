    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --cache --fix",
    "lint:style": "stylelint src/**/*/*.less --fix",
    "lint:all": "pnpm lint:ts && pnpm lint",
    "preinstall": "npx only-allow pnpm",
